<div class="row albumContent playList">
     <table class="table table-striped table-condensed">
        <thead>
            <tr>
                <td class="text-center">#</td>
                <td class="nombreTabla">Nombre</td>
                <!-- <td class="shareTable text-right">Agregar</td> -->
                <td class="text-left">Duraci√≥n</td>
                <!-- <td class="shareTable text-center">Compartir</td> -->
                <td class="shareTable text-center">&nbsp;</td>
            </tr>
        </thead>
        <tbody>
            {% for key, song in album.getSongs%}
                <tr>
                    <td class="text-center">{{ song.getTrackNumber }}</td>
                    <td class="nombreTabla songName"><a href="#" id="{{ album.getId }}-{{ song.getTrackNumber }}">{{ song.getName }}</a></td>
                    <!-- <td class="compiTable text-right"><a href="#"><i class="glyphicon glyphicon-plus-sign"></i></a></td> -->
                    <td class="text-left">{{ song.getLength}}</td>
                    <!-- <td class="shareTable text-center"><a href="#"><i class="glyphicon glyphicon-share"></i></a></td> -->               
                    <td class="shareTable text-center">&nbsp;</td>
                </tr>
            {% endfor %}
        </tbody>      
    </table>
</div>